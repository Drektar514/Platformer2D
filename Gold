using UnityEngine;

public class Gold : MonoBehaviour
{
    [SerializeField] private int _gold;
    
    private void OnTriggerEnter2D(Collider2D collision)
    {
        if(collision.TryGetComponent<Player>(out Player player))
        {
            player.GetGold(_gold);
            Destroy(gameObject);
        }
    }
}
